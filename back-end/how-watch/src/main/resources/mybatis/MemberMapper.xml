<mapper namespace="com.nareun.howwatch.repository.MemberMapper">
    <select id="findByEmail" resultType="domain.Member" parameterType="dto.MemberRequestDTO">
        SELCT * FROM MEMBER 
        WHERE email = #{email}
        AND password = #{password}
    </select>
</mapper>